      y Barbuda\"},\"ARG\":{\"id\":\"ARG\",\"value\":\"Argentina\"},\"ARM\":{\"id\":\"ARM\",\"value\":\"Armenia\"},\"ABW\":{\"id\":\"ABW\",\"value\":\"Aruba\"},\"AUS\":{\"id\":\"AUS\",\"value\":\"Australia\"},\"AUT\":{\"id\":\"AUT\",\"value\":\"Austria\"},\"AZE\":{\"id\":\"AZE\",\"value\":\"Azerbaiy\xE1n\"},\"BHS\":{\"id\":\"BHS\",\"value\":\"Bahamas\"},\"BHR\":{\"id\":\"BHR\",\"value\":\"Bare\xEDn\"},\"BGD\":{\"id\":\"BGD\",\"value\":\"Bangladesh\"},\"BRB\":{\"id\":\"BRB\",\"value\":\"Barbados\"},\"BLR\":{\"id\":\"BLR\",\"value\":\"Bielorrusia\"},\"BEL\":{\"id\":\"BEL\",\"value\":\"B\xE9lgica\"},\"BLZ\":{\"id\":\"BLZ\",\"value\":\"Belice\"},\"BEN\":{\"id\":\"BEN\",\"value\":\"Ben\xEDn\"},\"BMU\":{\"id\":\"BMU\",\"value\":\"Bermuda\"},\"BTN\":{\"id\":\"BTN\",\"value\":\"But\xE1n\"},\"BOL\":{\"id\":\"BOL\",\"value\":\"Bolivia\"},\"BES\":{\"id\":\"BES\",\"value\":\"Bonaire,
